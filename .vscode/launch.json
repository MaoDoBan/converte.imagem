{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno", // generated by using "Add configuration... > Deno: Run"
      "type": "pwa-node",
      "request": "launch",
      //"program": "${workspaceFolder}/test2.ts", // it seems "main.ts" is assumed, but the file might not exist
      //"program": "${file}",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-all", // unsafe default
        "src/test2.ts"
      ],
      "outputCapture": "std",
      "port": 33333
    }
  ]
}